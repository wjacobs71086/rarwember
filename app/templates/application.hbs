{{page-title "Rock & Roll with Octane"}}

<div class="bg-blue-900 text-gray-100 p-8 h-screen">
  <div class="mx-auto mb-4">
    <div class="h-12 flex items-center mb-4 border-b-2">
      <a href="#">
        <h1 class="font-bold text-2x1">
          Rock & Roll <span class="font-normal">with Octane</span>
        </h1>
      </a>
    </div>
    <ul>
      {{#each
        (array
          (hash title="Black Dog" band="Led Zeppelin" rating=3)
          (hash title="Yellow Ledbetter" band="Pearl Jam" rating=1)
          (hash title="The Pretender" band="The Foo Fighters" rating=5)
        ) as |song|
      }}
        <li>
          {{song.title}} by {{song.band}} ({{song.rating}})
        </li>
      {{/each}}
    </ul>
  </div>
</div>
